	<sql id="where_condition" >
		<dynamic prepend="WHERE">
			<isNotEmpty property="branchId" prepend="AND">
			FTZ_IN_MSG_CTL.BRANCH_ID = #branchId:VARCHAR#
			</isNotEmpty>
			<isNotEmpty property="msgNo" prepend="AND">
